</footer>\n  </div>\n</template>\n\n<script setup>\n// import { ref, onMounted } from 'vue';\nimport { fetchFooterIcons, fetchFooterText } from '~/composables/globalData.js';\nimport { msgTranslate, regLink, loginLink, loadLang } from '~/composables/globalData';\nimport { useCookieConsent } from '~/composables/useCookieConsent';\nconst { handleOpenPreferences } = useCookieConsent();\nconst { fetch, error, $fetchState } = useFetch(async () => {\n  await fetchFooterIcons(lang.value);\n  await fetchFooterText(lang.value);\n});\n\n// Add async data loading\nawait useAsyncData('translations', async () => {\n  try {\n    await loadLang();\n  } catch (error) {\n    console.error('Error loading translations:', error);\n  }\n});\n</script>\n\n<style scoped>\n.footer_logo {\n  background-color: #212121;\n  max-width: 150px;\n}\n</style>","export default \"data:image/svg+xml,%3csvg%20width='22'%20height='22'%20viewBox='0%200%2022%2022'%20fill='none'%20xmlns='http://www.w3.org/2000/svg'%3e%3crect%20width='22'%20height='22'%20rx='0.785714'%20fill='%23212633'/%3e%3crect%20x='3.6665'%20y='4.58325'%20width='9.16667'%20height='1.83333'%20rx='0.916667'%20fill='%239498A2'/%3e%3crect%20x='3.6665'%20y='10.0833'%20width='14.6667'%20height='1.83333'%20rx='0.916667'%20fill='%239498A2'/%3e%3crect%20x='3.6665'%20y='15.5833'%20width='11.9167'%20height='1.83333'%20rx='0.916667'%20fill='%239498A2'/%3e%3c/svg%3e\"","<template>\n  <!-- Cookie Banner -->\n  <div v-if=\"showBanner\" class=\"fixed bottom-0 left-0 right-0 bg-white p-4 border-t z-50\">\n    <div class=\"max-w-6xl mx-auto\">\n      <div class=\"flex flex-col md:flex-row items-center justify-between gap-4\">\n        <div class=\"space-y-2\">\n          <p class=\"text-xs text-gray-600 max-w-3xl\">\n            We use cookies to ensure our partners are properly credited when you visit gaming sites. This is essential for our service operation and partner payments. We also use analytics cookies to improve our service.\n          </p>\n          <p class=\"text-xs text-gray-500\">\n            Read our \n            <NuxtLink to=\"/compliance/cookies\" class=\"underline hover:text-gray-700\">Cookie Policy</NuxtLink>\n            and\n            <NuxtLink to=\"/compliance/privacy\" class=\"underline hover:text-gray-700\">Privacy Policy</NuxtLink>\n            for more information.\n          </p>\n        </div>\n        <div class=\"flex gap-2\">\n          <button \n            class=\"bg-black hover:bg-gray-800 text-white px-4 py-1.5 rounded text-xs\"\n            @click=\"handleAcceptAll\"\n          >\n            Accept All\n          </button>\n          <button \n            class=\"border border-gray-300 hover:bg-gray-50 text-gray-700 px-4 py-1.5 rounded text-xs\"\n            @click=\"handleDeclineAll\"\n          >\n            Decline Optional Cookies\n          </button>\n          <button \n            class=\"border border-gray-300 hover:bg-gray-50 text-gray-700 px-4 py-1.5 rounded text-xs\"\n            @click=\"handleOpenPreferences\"\n          >\n            Manage Preferences\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <!-- Cookie Preferences Modal - Always mounted, shown based on isPreferencesOpen -->\n  <Teleport to=\"body\">\n    <div v-if=\"isPreferencesOpen\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-[100]\">\n      <div class=\"bg-white rounded-lg p-6 max-w-xl w-full mx-4\">\n        <div class=\"flex justify-between items-center mb-4\">\n          <h2 class=\"text-sm font-normal text-gray-900\">\n            Cookie Preferences\n          </h2>\n          <button @click=\"isPreferencesOpen = false\" class=\"text-gray-400 hover:text-gray-600 text-sm\">\n            ✕\n          </button>\n        </div>\n        \n        <div class=\"space-y-6\">\n          <div v-for=\"category in cookieCategories\" :key=\"category.id\" class=\"space-y-1\">\n            <div class=\"flex items-center justify-between\">\n              <h4 class=\"text-xs font-medium text-gray-900\">{{ category.label }}</h4>\n              <label class=\"relative inline-flex items-center cursor-pointer\">\n                <input\n                  type=\"checkbox\"\n                  v-model=\"preferences[category.id]\"\n                  :disabled=\"category.required\"\n                  class=\"sr-only peer\"\n                >\n                <div class=\"w-9 h-5 bg-gray-200 peer-focus:outline-none rounded-full peer \n                           peer-checked:after:translate-x-full peer-checked:after:border-white \n                           after:content-[''] after:absolute after:top-[2px] after:left-[2px] \n                           after:bg-white after:border-gray-300 after:border after:rounded-full \n                           after:h-4 after:w-4 after:transition-all peer-checked:bg-black\">\n                </div>\n              </label>\n            </div>\n            <p class=\"text-xs text-gray-500\">{{ category.description }}</p>\n          </div>\n\n          <div class=\"text-xs text-gray-500 mb-4\">\n            For more information about how we use cookies, please read our \n            <NuxtLink to=\"/compliance/cookies\" class=\"underline hover:text-gray-700\">Cookie Policy</NuxtLink>\n            and\n            <NuxtLink to=\"/compliance/privacy\" class=\"underline hover:text-gray-700\">Privacy Policy</NuxtLink>.\n          </div>\n\n          <button \n            class=\"w-full bg-black hover:bg-gray-800 text-white px-4 py-1.5 rounded text-xs\"\n            @click=\"savePreferences\"\n          >\n            Save Preferences\n          </button>\n        </div>\n      </div>\n    </div>\n  </Teleport>\n</template>\n\n<script setup>\nimport { useCookieConsent } from '~/composables/useCookieConsent';\n\nconst {\n  showBanner,\n  isPreferencesOpen,\n  preferences,\n  cookieCategories,\n  handleAcceptAll,\n  handleDeclineAll,\n  savePreferences,\n  handleOpenPreferences\n} = useCookieConsent();\n</script>\n","<template>\n    <div class=\"\">\n        <nav class=\"bg-primary_bg fixed z-10 py-4 shadow-lg items-center w-full\">\n          <div class=\"w-full md:px-0\">\n            <div class=\"container md:mx-auto grid grid-cols-3 items-center\">\n                <div class=\"left\">\n                  <div class=\"menu-btn items-center\" id=\"menu-btn\">\n                        <img src=\"../assets/hamburgerMenu.svg\" alt=\"Mobile Menu Button\" class=\"w-9 h-9 pl-4 items-center\">\n                          <div class=\"menu bg-secondary_bg rounded text-left w-36 drop-shadow-[0_15px_15px_rgba(0,0,0,0.50)] transition duration-300 ease-in-out transform scale-0 origin-top\" id=\"menu\">\n                            <NuxtLink to=\"/\" class=\"menu-item hover:bg-slate-800 px-5\">{{ msgTranslate?.home || 'Home' }}</NuxtLink>\n                            <NuxtLink to=\"/promotions\" class=\"menu-item hover:bg-slate-800 px-5\">{{ msgTranslate?.promotions || 'Promotions' }}</NuxtLink>\n                            <NuxtLink to=\"/compliance\" class=\"menu-item hover:bg-slate-800 px-5\">{{ msgTranslate?.legal || 'Legal' }}</NuxtLink>\n                            <NuxtLink to=\"/all-games\" class=\"menu-item hover:bg-slate-800 px-5\">{{ msgTranslate?.all_games || 'All Games' }}</NuxtLink>\n                            <NuxtLink to=\"/popular-games\" class=\"menu-item hover:bg-slate-800 px-5\">{{ msgTranslate?.popular_games || 'Popular Games' }}</NuxtLink>\n                            <NuxtLink to=\"/slot-games\" class=\"menu-item hover:bg-slate-800 px-5\">{{ msgTranslate?.slot_games || 'Slot Games' }}</NuxtLink>\n                            <NuxtLink to=\"/casino-games\" class=\"menu-item hover:bg-slate-800 px-5\">{{ msgTranslate?.casino_games || 'Casino Games' }}</NuxtLink>\n                            <NuxtLink to=\"/jackpot-games\" class=\"menu-item hover:bg-slate-800 px-5\">{{ msgTranslate?.jackpot_games || 'Jackpot Games' }}</NuxtLink>\n          